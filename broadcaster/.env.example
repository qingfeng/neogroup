# Bearer token for authenticating requests from Cloudflare Worker
# Must match the NOSTR_BRIDGE_TOKEN secret in Cloudflare Worker
BRIDGE_TOKEN=your-secret-token-here

# Comma-separated list of Nostr relay WebSocket URLs
RELAY_LIST=wss://relay.damus.io,wss://nos.lol,wss://relay.nostr.band

# HTTP port
PORT=3000
